
public class AtomicExpression extends Expression{
	private double num;
	//constructor that getting one variable of double type 
	public AtomicExpression(double n){	
		this.num = n;
	}
	//getter of the variable num
	public double getNum() {
		return this.num;
	}
	//calculate the number ,the expression is containing one number therefor return the value of the number
	public double calculate(){		
		return num;
	}
	//override the toString method
	public String toString(){
		return "" + this.getNum();
	}

}
